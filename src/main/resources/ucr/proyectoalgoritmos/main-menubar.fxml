<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.*?>

<MenuBar xmlns:fx="http://javafx.com/fxml/1"
         fx:controller="ucr.proyectoalgoritmos.Controller.MainMenuController">
    <menus>
        <Menu
                mnemonicParsing="false"
                text="Log In/Sign Up">
            <items>
                <MenuItem
                        fx:id="loginSettings"
                        mnemonicParsing="false"
                        onAction="#loadLoginPane"
                        text="User Registration"/>
                <MenuItem
                        fx:id="menuExit"
                        mnemonicParsing="false"
                        onAction="#exit"
                        text="Exit"/>
            </items>
        </Menu>
    </menus>
</MenuBar>

